{
  "name": "gdu-common",
  "version": "0.3.0",
  "description": "a library to support the apps at https://GameDevUtils.com",
  "repository": "GameDevUtils/gdu-sheets",
  "files": [
    "build"
  ],
  "main": "build/index",
  "types": "build/index.d.ts",
  "bin": "build/index.js",
  "type": "node",
  "scripts": {
    "mjs": "tsc -d && mv build/index.js build/index.cjs",
    "cjs": "tsc -m commonjs",
    "clean": "rm -rf ./build/",
    "build": "tsc -d && tsc -m commonjs && cp -r ./src/_assets ./build",
    "rebuild": "npm run clean && npm run build",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "watch": "jest --watchAll --coverage",
    "skipped": "jest ./src --reporters jest-skipped-reporter",
    "licenses": "license-report --output=table --only=prod",
    "circular": "dependency-cruise --config -- src/",
    "circular-old": "madge --warning --circular --extensions ts,tsx,js,jsx --ts-config ./tsconfig.json src/"
  },
  "keywords": [],
  "author": "Joseph B. Hall (@groundh0g)",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@types/bmp-js": "^0.1.0",
    "@types/jest": "^27.0.3",
    "@types/js-base64": "^3.3.1",
    "@types/ndarray": "^1.0.11",
    "@types/node": "^16.7.10",
    "@types/pngjs": "^6.0.1",
    "@types/uuid": "^8.3.4",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^27.3.1",
    "browserify-zlib": "^0.2.0",
    "crypto-browserify": "^3.12.0",
    "dependency-cruiser": "^12.3.0",
    "eslint": "^7.32.0",
    "https-browserify": "^1.0.0",
    "jest": "^27.3.1",
    "jest-skipped-reporter": "^0.0.5",
    "license-report": "^6.3.0",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "pkg": "^5.3.1",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "bmp-js": "^0.1.0",
    "decode-gif": "^1.0.1",
    "jpeg-js": "^0.4.3",
    "jszip": "^3.9.1",
    "ndarray": "^1.0.19",
    "pngjs": "^6.0.0",
    "save-pixels": "^2.3.6",
    "uuid": "^8.3.2"
  }
}
